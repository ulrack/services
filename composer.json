{
    "name": "ulrack/services",
    "description": "A services implementation for PHP applications.",
    "keywords": ["services", "dependency-injection"],
    "type": "library",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "grizz-it/configuration": "^1.0",
        "grizz-it/object-factory": "^1.1",
        "grizz-it/storage": "^1.0",
        "grizz-it/validator": "^1.0"
    },
    "suggest": {
        "ulrack/json-schema": "Generate validators with JSON Schema's"
    },
    "authors": [{
        "name": "Ulrack",
        "homepage": "https://www.ulrack.com/",
        "role": "Developer"
    }],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Ulrack\\Services\\": "src/"
        },
        "files": [
            "locator.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ulrack\\Services\\Tests\\": "tests/"
        }
    },
    "archive": {
        "exclude": [
            "/example",
            "/tests",
            "/.gitignore",
            "/.travis.yml",
            "/phpunit.xml",
            "/phpcs.xml",
            "/PULL_REQUEST_TEMPLATE.md",
            "/CODE_OF_CONDUCT.md",
            "/CONTRIBUTING.md"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.4"
    }
}
